{
  "family": "mythicalmysfitsservice",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::728610231177:role/MythicalMysfitsCoreStack-EcsServiceRole-184GDKSHK6SB8",
  "taskRoleArn": "arn:aws:iam::728610231177:role/MythicalMysfitsCoreStack-ECSTaskRole-W1OULT4YDEDV",
  "containerDefinitions": [
    {
      "name": "MythicalMysfits-Service",
      "image": "728610231177.dkr.ecr.eu-central-1.amazonaws.com/mythicalmysfits/service:lates",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "mythicalmysfits-logs",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "awslogs-mythicalmysfits-service"
        }
      },
      "essential": true
    }
  ]
}